<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.burningbuilding">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BurningBuilding">
        <activity android:name=".EndGameActivity"></activity>
        <activity android:name=".Floor10Activity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".RecordsActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".LoadingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".SoundServiceElevator"  android:enabled="true"></service>
        <service android:name=".SoundFire"  android:enabled="true"></service>
    </application>
</manifest>